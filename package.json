{
  "name": "another-angular-webpack-starter",
  "version": "1.0.0",
  "description": "Another webpack starter for Angular (includes SASS & Bootstrap)",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 1841",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack/webpack.prod.js --progress --profile --bail",
    "build-development": "webpack --config config//webpack/webpack.development.js --progress --profile --bail",
    "production": "lite-server -c config/lite-server/lite-server-config.production.js",
    "development": "lite-server -c config/lite-server/lite-server-config.development.js"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.1",
    "@angular/compiler": "~2.4.1",
    "@angular/core": "~2.4.1",
    "@angular/forms": "~2.4.1",
    "@angular/http": "~2.4.1",
    "@angular/platform-browser": "~2.4.1",
    "@angular/platform-browser-dynamic": "~2.4.1",
    "@angular/router": "~3.4.1",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.4.1",
    "ng2-bootstrap": "^1.1.16-11",
    "rxjs": "5.0.2",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.40",
    "@types/node": "^6.0.55",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "css-loader": "^0.26.1",
    "css-to-string-loader": "^0.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.25.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-scss-preprocessor": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "node-sass": "^4.1.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^1.2.1",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "typescript": "~2.1.4",
    "typedoc": "~0.5.3",
    "typings": "^2.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^2.0.0",
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2"
  }
}
